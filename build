#!/bin/bash
cd /tmp/rom # Depends on where source got synced

rm -rf out

# Normal build steps
. build/envsetup.sh
lunch xdroid_X01BD-userdebug
export USE_CCACHE=1
ccache -M 60G
make xd -j8 aapt

brunch xdroid_X01BD-userdebug

curl --upload-file ./out/target/product/X01BD/*.zip  https://transfer.sh/xdroid.zip
