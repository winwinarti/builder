  #!/bin/bash
mkdir -p /tmp/rom # Where to sync source
cd /tmp/rom

# Initialize local repository
repo init -u https://github.com/xdroid-CAF/xd_manifest -b eleven  --depth=1

# Sync
repo sync -c -j8 --force-sync --no-clone-bundle --no-tags

git clone https://github.com/ramadhannangga/device_asus_X01BD device/asus/X01BD
git clone https://github.com/ramadhannangga/device_asus_sdm660-common device/asus/sdm660-common
git clone https://github.com/ramadhannangga/vendor_asus vendor/asus
git clone https://github.com/ThankYouMario/proprietary_vendor_qcom_sdclang.git -b ruby-12 prebuilts/clang/host/linux-x86/clang-sdllvm
git clone  https://github.com/ramadhannangga/msm-4.4 kernel/asus/sdm660
